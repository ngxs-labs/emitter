{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "dispatcher",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "dispatcher",
      "kebab-case"
    ],
    "no-big-function": false,
    "no-duplicate-string": false,
    "no-non-null-assertion": false
  }
}
